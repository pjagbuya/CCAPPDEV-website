<div class="reserve-edit">
    <div class="inner-box">
      <div class="exit-section d-flex flex-start flex-row">
        <a href="LT-user-room-edit.html">
          <button class="btn btn-danger my-3" type="button" name="button">Exit Seat Editor</button>
        </a>

      </div>
      <h1>Seat Editor</h1>
      Room No.: <span id="room-name-ID">{{labName}}</span>
      <button>Change</button><br>
        <div class="inner-box-table">
          <table>
            <thead>
              <tr>
                <th class="header tc">Date<img src="https://www.svgrepo.com/show/42233/pencil-edit-button.svg" style="width: 20px"></th>
                <th class="header tc">Time Slot<img src="https://www.svgrepo.com/show/42233/pencil-edit-button.svg" style="width: 20px"></th>
                <th class="header tc">Seat No.<img src="https://www.svgrepo.com/show/42233/pencil-edit-button.svg" style="width: 20px"></th>
              </tr>
            </thead>
            <tbody>
              {{#each data}}
                <tr>
                  <td class="tc">{{firstSeat.weekDay}}<br><button class="change-btn-weekday">Change</button></td>
                  <td class="tc">
                    {{firstSeat.timeInterval}}<br>
                    <select class="select-interval">
                      <option>7:30 A.M. - 8:00 A.M.</option>
                      <option>8:00 A.M. - 8:30 A.M.</option>
                      <option>8:30 A.M. - 9:00 A.M.</option>
                      <option>9:00 A.M. - 9:30 A.M.</option>
                      <option>9:30 A.M. - 10:00 A.M.</option>
                      <option>10:00 A.M. - 10:30 A.M.</option>
                      <option>10:30 A.M. - 11:00 A.M.</option>
                      <option>11:00 A.M. - 11:30 A.M.</option>
                      <option>11:30 A.M. - 12:00 P.M.</option>
                      <option>12:00 P.M. - 12:30 P.M.</option>
                      <option>12:30 P.M. - 1:00 P.M.</option>
                      <option>1:00 P.M. - 1:30 P.M.</option>
                      <option>1:30 P.M. - 2:00 P.M.</option>
                      <option>2:00 P.M. - 2:30 P.M.</option>
                      <option>2:30 P.M. - 3:00 P.M.</option>
                      <option>3:00 P.M. - 3:30 P.M.</option>
                      <option>3:30 P.M. - 4:00 P.M.</option>
                      <option>4:00 P.M. - 4:30 P.M.</option>
                      <option>4:30 P.M. - 5:00 P.M.</option>
                      <option>5:00 P.M. - 5:30 P.M.</option>
                    </select>
                  </td>
                  <td class="tc">
                    {{firstSeat.seatNumber}}
                    <br><button class="change-btn-seatnumber">Change</button>
                  </td>
                  <td class="tc"><br><button class="btn btn-danger cancel-btn-seatnumber">Cancel Seat</button></td>
                </tr>
                {{#each otherSeats}}
                  <tr>
                    <td class="tc">{{weekDay}}<br><button class="change-btn-weekday">Change</button></td>
                    <td class="tc">
                      {{timeInterval}}<br>
                      <select class="select-interval">
                        <option>7:30 A.M. - 8:00 A.M.</option>
                        <option>8:00 A.M. - 8:30 A.M.</option>
                        <option>8:30 A.M. - 9:00 A.M.</option>
                        <option>9:00 A.M. - 9:30 A.M.</option>
                        <option>9:30 A.M. - 10:00 A.M.</option>
                        <option>10:00 A.M. - 10:30 A.M.</option>
                        <option>10:30 A.M. - 11:00 A.M.</option>
                        <option>11:00 A.M. - 11:30 A.M.</option>
                        <option>11:30 A.M. - 12:00 P.M.</option>
                        <option>12:00 P.M. - 12:30 P.M.</option>
                        <option>12:30 P.M. - 1:00 P.M.</option>
                        <option>1:00 P.M. - 1:30 P.M.</option>
                        <option>1:30 P.M. - 2:00 P.M.</option>
                        <option>2:00 P.M. - 2:30 P.M.</option>
                        <option>2:30 P.M. - 3:00 P.M.</option>
                        <option>3:00 P.M. - 3:30 P.M.</option>
                        <option>3:30 P.M. - 4:00 P.M.</option>
                        <option>4:00 P.M. - 4:30 P.M.</option>
                        <option>4:30 P.M. - 5:00 P.M.</option>
                        <option>5:00 P.M. - 5:30 P.M.</option>
                      </select>
                    </td>
                    <td class="tc">
                      {{seatNumber}}<br><button class="change-btn-seatnumber">Change</button></td>
                    <td class="tc"><br><button class="btn btn-danger cancel-btn-seatnumber">Cancel Seat</button></td>
                  </tr>
                {{/each}}
              {{/each}}

            </tbody>
          </table>
        </div>
    </div>
</div>
